rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if
      		request.auth.uid != null && request.time < timestamp.date(2021, 1, 25);
      allow write: if
      		request.auth.uid != null && request.time < timestamp.date(2021, 1, 25);
    }
  }
}